{
  "name": "botkit-karma-simple",
  "description": "increment things",
  "version": "0.3.0",
  "author": "@9renpoto",
  "bugs": {
    "url": "https://github.com/9renpoto/botkit-karma-simple/issues"
  },
  "dependencies": {
    "karma-store-redis": "^0.3.0"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "4.7.0",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "botkit": "0.7.4",
    "esdoc": "1.1.0",
    "esdoc-flow-type-plugin": "1.1.0",
    "eslint": "5.15.1",
    "flow-bin": "0.94.0",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.3"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/9renpoto/botkit-karma-simple#readme",
  "keywords": [
    "karma"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ],
    "*.md": [
      "prettier --write --parser markdown",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "peerDependencies": {
    "botkit": "^0.6.x || ^0.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/9renpoto/botkit-karma-simple.git"
  },
  "scripts": {
    "build": "babel src -d lib && esdoc",
    "clean": "rimraf lib coverage",
    "deploy": "gh-pages -d doc",
    "lint": "run-p lint:*",
    "lint:flow": "flow",
    "lint:js": "eslint . --cache",
    "postpublish": "npm run deploy",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "start": "npm run build && node example/slack.js",
    "test": "npm run lint && npm run build"
  }
}
